# Production Environment Configuration for Frontend
# Copy this file to .env.production and configure for your deployment

# API Configuration
# The backend API URL - set this if backend is on a different domain
# Leave empty if frontend and backend are on the same domain
VITE_API_URL=

# Socket.IO URL (usually same as API URL)
# Leave empty to use the same origin as the frontend
VITE_SOCKET_URL=

# Backend Port (only used in development)
VITE_BACKEND_PORT=5001

# App Configuration
VITE_APP_NAME=EventHub
VITE_APP_DESCRIPTION=Your Event Management Platform
